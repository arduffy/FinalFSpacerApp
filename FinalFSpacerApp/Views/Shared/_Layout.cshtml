                    <div class="list-group">
                        <a href="/" class="list-group-item active text-center">
                            <h4 class="glyphicon glyphicon-home"></h4><br />Home
                        </a>
                        <a href="/drivecards" class="list-group-item text-center">
                            <h4 class="glyphicon glyphicon-file"></h4><br />Drive Cards
                        </a>
                        <a href="/spacercards/FrontCard" class="list-group-item text-center">
                            <h4 class="glyphicon glyphicon-info-sign"></h4><br />Complete FSpacer Form
                        </a>
                        <a href="/drivecards/search" class="list-group-item text-center">
                            <h4 class="glyphicon glyphicon-search"></h4><br />Search
                        </a>
                    </div>
                </div>
            @*</div>*@

        </div>

    @*</div>*@
        </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2017 - FinalFSpacerApp</p>
        </footer>
    </div>
